{
  "name": "kb-clone",
  "version": "0.0.0",
  "scripts": {
    "start:ui": "ng serve",
    "start:functions": "cd functions && npm run serve",
    "build": "NODE_ENV=production ng build --prod=true",
    "build:stats": "NODE_ENV=production ng build --prod=true --stats-json",
    "analyze": "webpack-bundle-analyzer dist/kb-clone/stats.json",
